{
  "name": "majidmvulle/storage-bundle",
  "license": "None",
  "type": "library",
  "description": "Library for storage of files in the file system or CDN and reference the record in the database.",
  "authors": [
    {
      "name": "Majid Mvulle",
      "email": "majid@majidmvulle.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "MajidMvulle\\Bundle\\StorageBundle\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.1",
    "aws/aws-sdk-php": "^3.55",
    "doctrine/doctrine-bundle": "^1.8.1",
    "doctrine/orm": "^2.6.0",
    "friendsofsymfony/user-bundle": "^2.1.2",
    "jms/serializer-bundle": "^3.0",
    "knplabs/gaufrette": "^0.7.0",
    "ramsey/uuid-doctrine": "^1.4",
    "sonata-project/user-bundle": "^4.1",
    "symfony/console": "^4.0"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^4.0"
  },
  "config": {
    "platform": {
      "php": "7.2.0"
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  }
}
